    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.spaniernathan.onemoreshot",
      "supportsTablet": true
    },
    "android": {
      "package": "com.spaniernathan.onemoreshot"
    }
  }
}